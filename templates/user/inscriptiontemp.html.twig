        <label for="name">Nom :</label>
        <input type="text" id="name" name="name" required>
    </div>
    <div class="form-group">
        <label for="firstname">Prenom :</label>
        <input type="text" id="firstname" name="firstname" required>
    </div>
    <div class="form-group">
        <label for="email">e-mail :</label>
